        <JDBCSampler guiclass="TestBeanGUI" testclass="JDBCSampler" testname="Persons with most likes per tag">
          <stringProp name="dataSource">bss</stringProp>
          <stringProp name="queryType">Select Statement</stringProp>
          <stringProp name="query">WITH LikesPerTag AS (
    SELECT 
        person_id, 
        tag_id, 
        COUNT(*) as totalLikes
    FROM 
        post_like 
        JOIN post_tag USING (post_id)
    GROUP BY person_id, tag_id
),
MaxLikesPerTag AS (
    SELECT 
        tag_id, 
        MAX(totalLikes) AS maxLikes
    FROM 
        LikesPerTag
    GROUP BY tag_id
)
SELECT 
    person_id, 
    LikesPerTag.tag_id, 
    totalLikes
FROM 
    LikesPerTag
    JOIN MaxLikesPerTag 
    ON (LikesPerTag.tag_id = MaxLikesPerTag.tag_id AND LikesPerTag.totalLikes = MaxLikesPerTag.maxLikes)
;</stringProp>
